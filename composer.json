{
    "name": "motivo/editorjs-data-converter",
    "description": "A package to convert JSON data that Editor.js generates back to html so it can be used.",
    "keywords": [
        "laravel",
        "editorjs",
        "motivo"
    ],
    "authors": [
        {
            "name": "Wouter van Marrum",
            "email": "wouter@woutervm.com",
            "homepage": "https://motivo.nl",
            "role": "developer"
        },
        {
            "name": "Jelle",
            "email": "jelle@motivo.nl",
            "homepage": "https://motivo.nl"
        }
    ],
    "type": "library",
    "license": "mpl-2.0",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Motivo\\EditorJsDataConverter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Motivo\\EditorJsDataConverter\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "illuminate/support": "~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "spatie/laravel-html": "^2.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Motivo\\EditorJsDataConverter\\EditorJsServiceProvider"
            ]
        }
    },
    "scripts": {
    }
}
